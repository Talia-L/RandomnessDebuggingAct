import java.util.Random;

public class RandomGeneratorAct
{
    public static void main(String[] args)
    {
        Random r = new Random();

        System.out.print("Generate 10 random integers between 0 and 6 ");
        
        for(int i = 1; i <= 12; ++i)
        
          {int randomInt = 1 + r.nextInt(7);
          output("Generated number: " + randomInt);}
        
    
        output("Done.");
    }
